{
	"compilerOptions": {
		"target": "ES2022",
		"jsx": "react",
		"module": "ES2022",
		"moduleResolution": "bundler",
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"strictPropertyInitialization": false,
		"skipLibCheck": true,
		"noImplicitAny": false,
		"lib": ["ES2022", "DOM"]
	},
	"include": ["src/**/*"]
}
